{
  "extends": "stacks",
  "globals": {
	"FS": true
  },
  "env": {
	"browser": true
  },
  "rules": {
	"import/extensions": [
	  0
	],
	"linebreak-style": [
	  0
	],
	"max-len": [
	  2,
	  120,
	  2,
	  {
		"ignoreUrls": true,
		"ignoreComments": false
	  }
	],
	"no-nested-ternary": [
	  0
	],
	"import/no-unresolved": [
	  0
	],
	"global-require": [
	  0
	],
	"arrow-body-style": [
	  0
	],
	"react/prop-types": [
	  0
	],
	"react/require-default-props": [
	  0
	],
	"import/no-extraneous-dependencies": [
	  0
	],
	"arrow-parens": [
	  0
	]
  },
  "settings": {
	"import/resolver": {
	  "babel-module": {}
	}
  }
}
